'use strict';

module.exports.setup = function(server, proxy) {

  /**
   * @swagger
   * {{ routeUrl }}:
   *   {{ method }}:
   *   description: {{ swaggerConfig.description }}
   *   produces:
   *    - {{ swaggerConfig.produces }}
   *   parameters:
   *   {{#each swaggerConfig.parameters}} - name: {{ name }}
   *   {{#each props}}{{#pairs .}}   {{key}}: {{value}}{{/pairs}}
   *   {{/each}}
   *   {{/each}}
   *   responses:
   *   {{#each swaggerConfig.responses}}    {{ status }}: {{ description }}
   *   {{/each}}
   */
  server.{{ method }}('{{ routeUrl }}', (req, res, next) => {
    proxy.web(req, res);
    return next();
  });

}
